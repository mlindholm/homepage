{% if site.env == "prod" %}
<script async src="https://www.googletagmanager.com/gtag/js?id={{ site.gaid }}"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());
  gtag('config', '{{ site.gaid }}');
</script>
{% endif %}